@model CentroVacunacion2

<form method="post" asp-action="BorrarPost">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Borrar Centro de Vacunación</h2>
            <hr />
        </div>

        @*<div asp-validation-summary="All"></div>*@
        <div class="mb-3">
            <label asp-for="Descripcion"></label>
            <input asp-for="Descripcion" disabled class="form-control" />
            <span asp-validation-for="Descripcion" class="text-danger"></span>
        </div>

        <div class="mb-3">
            <label asp-for="ProvinciaId"></label>
            <input asp-for="ProvinciaId" disabled class="form-control" />
            <span asp-validation-for="ProvinciaId" class="text-danger"></span>
        </div>

        <button type="submit" class="btn btn-danger" style="width:150px">Borrar</button>
        <a asp-controller="CentroVacunacion" asp-action="Index" class="btn btn-secondary" style="width:150px">
            Regresar
        </a>
    </div>
</form>

@section Scripts{
    @{
        <partial name="_ValidationScriptsPartial" />
    }
}



